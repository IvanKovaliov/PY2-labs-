using System;
using System.Linq;

public class LatinLettersArray
{
    private char[] array;

    public LatinLettersArray(int n)
    {
        Random random = new Random();
        array = new char[n];
        for (int i = 0; i < n; i++)
        {
            array[i] = (char)random.Next(65, 91); // Генерация прописных латинских букв
        }
    }

    public void SwapSecondAndSecondLast()
    {
        if (array.Length < 2)
            return;

        int secondIndex = 1;
        int secondLastIndex = array.Length - 2;

        char temp = array[secondIndex];
        array[secondIndex] = array[secondLastIndex];
        array[secondLastIndex] = temp;
    }

    public int LastIndexOfY()
    {
        for (int i = array.Length - 1; i >= 0; i--)
        {
            if (array[i] == 'Y')
                return i;
        }
        return -1; // Если символ 'Y' не найден
    }

    public int ConsonantCount
    {
        get
        {
            char[] consonants = { 'B', 'C', 'D', 'F', 'G', 'H', 'J', 'K', 'L', 'M', 'N', 'P', 'Q', 'R', 'S', 'T', 'V', 'W', 'X', 'Y', 'Z' };
            return array.Count(c => consonants.Contains(c));
        }
    }

    public void PrintArray()
    {
        Console.WriteLine(string.Join(" ", array));
    }
}

class Program
{
    static void Main(string[] args)
    {
        LatinLettersArray array = new LatinLettersArray(10);
        Console.WriteLine("Исходный массив:");
        array.PrintArray();

        array.SwapSecondAndSecondLast();
        Console.WriteLine("Массив после обмена второго и предпоследнего элементов:");
        array.PrintArray();

        int lastYIndex = array.LastIndexOfY();
        Console.WriteLine(lastYIndex != -1 ? $"Индекс последнего 'Y': {lastYIndex}" : "Символ 'Y' не найден");

        Console.WriteLine($"Количество согласных букв: {array.ConsonantCount}");
    }
}